{
  "env": {
    "browser": true,
    "es2021": true
  },
  "plugins": ["prettier", "@typescript-eslint"],
  "extends": ["next/core-web-vitals", "plugin:storybook/recommended", "plugin:@typescript-eslint/recommended", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "no-debugger": 0,
    "max-len": 0,
    "no-unused-vars": 0
  }
}
